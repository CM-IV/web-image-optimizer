{
  "name": "electron-vite-react",
  "productName": "Electron",
  "private": true,
  "version": "0.4.1",
  "description": "Vite Preact Electron boilerplate.",
  "author": "CM-IV <chuck@civdev.xyz>",
  "license": "MIT",
  "main": "dist-electron/electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "sharp-rebuild-linux": "npm rebuild --platform=linux sharp",
    "build-linux": "tsc && vite build && electron-builder -l",
    "sharp-rebuild-win": "npm rebuild --platform=win32 sharp",
    "build-win": "tsc && vite build && electron-builder -w"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "devDependencies": {
    "@formkit/auto-animate": "1.0.0-beta.3",
    "@preact/preset-vite": "^2.4.0",
    "@types/sharp": "^0.31.0",
    "electron": "^21.1.0",
    "electron-builder": "^23.3.3",
    "preact": "^10.11.2",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vite-electron-plugin": "^0.4.6",
    "wouter-preact": "2.8.0-alpha.2"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777"
    }
  },
  "dependencies": {
    "sharp": "^0.31.1"
  }
}
